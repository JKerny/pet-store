@model List<PetStore.Areas.Admin.Models.Identity.UsersViewModel>
@{
    ViewBag.Title = "Users";
    Layout = "~/areas/admin/Views/Shared/_Layout.cshtml";
}

<div>
    @Html.ActionLink("Create New User", "Register", "Account")
</div>

<table class="table table-striped">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Role</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Role</td>
            </tr>

        }
    </tbody>
</table>



